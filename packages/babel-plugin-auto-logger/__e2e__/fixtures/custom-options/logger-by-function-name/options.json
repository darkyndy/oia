{
  "plugins": [
    [
      "./../../../../lib/",
      {
        "loggingData": {
          "levelForTryCatch": "error",
          "levelForMemberExpressionCatch": "error",
          "levels": {
            "debug": {
              "matchFunctionName": "sum",
              "methodName": "myDebug"
            },
            "error": {
              "matchFunctionName": "it-is-ignored-for-error-level",
              "methodName": "myError"
            },
            "info": {
              "matchFunctionName": "sub",
              "methodName": "myInfo"
            },
            "log": {
              "matchFunctionName": "division",
              "methodName": "myLog"
            },
            "warn": {
              "matchFunctionName": "multiply",
              "methodName": "myWarn"
            }
          },
          "name": "myLogger",
          "source": "path/to/file"
        }
      }
    ]
  ]
}